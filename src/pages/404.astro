---

import Layout from "../layouts/Layout.astro"

import NewsletterSectionExternalForm from "../components/NewsletterSectionExternalForm.astro"
import newsletter from "../data/newsletter.json"

---

<Layout title="Page Not Found!">

  <section class="max-w-[92vw] lg:max-w-4xl mx-auto bs-mt-md px-5 py-8 md:p-20 flex flex-col gap-3 justify-between border rounded-md bg-bs-surface-2 border-bs-surface-3 [&_a]:underline [&_a]:underline-offset-4 [&_a]:decoration-bs-foreground-dark/80 [&_a]:hover:decoration-bs-foreground-light">

    <!-- Title -->
    <h1 class="bs-h1">Not Found!</h1>
    There is no content at this url... please try a different one... <a href="/">Like the homepage</a> or <a href="/blog">Our Blog</a>


  </section>
    <NewsletterSectionExternalForm newsletter={newsletter} />

</Layout>
